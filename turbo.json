{
  "pipeline": {
    "prebuild:content": {
      "cache": false
    },
    "build": {
      "dependsOn": ["prebuild:content", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:localnet": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "link": {
      "dependsOn": ["build"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:program": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "clean": {
      "cache": false
    },
    "content:prepublish": {},
    "content:publish": {}
  }
}
