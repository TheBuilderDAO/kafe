{
  "name": "@builderdao/program-tutorial",
  "version": "0.0.6",
  "description": "BuilderDAO SDK for interacting with the DAO program",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./ts-sdk/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "module": "./dist/index.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "Solana",
    "DAO",
    "Anchor",
    "Typescript",
    "Rust"
  ],
  "license": "MIT",
  "author": "@builderdao",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "idl:copy": "cp -fv ../../target/types/tutorial.ts ts-sdk/lib/idl && cp -fv ../../target/idl/tutorial.json ts-sdk/lib/idl",
    "build": "tsup ts-sdk/index.ts --format esm,cjs --dts",
    "dev": "yarn build --watch",
    "lint": "TIMING=1 eslint ts-sdk --fix",
    "clean": "rm -rf node_modules && rm -rf dist",
    "test:program": "jest --coverage --forceExit"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.23.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.32.0",
    "bs58": "^4.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@builderdao/config": "*",
    "@builderdao/tsconfig": "*",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "eslint": "^8.5.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
